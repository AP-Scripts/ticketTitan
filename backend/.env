# Your bot token from the Discord Developer Portal
BOT_TOKEN=your-discord-bot-token-here

# Your bot's application client ID (for slash command registration if needed)
CLIENT_ID=your-bot-client-id

CLIENT_SECRET=your-client-secret

OAUTH_CALLBACK_URL=http://localhost:3001/callback

SESSION_SECRET=your-super-secret

# Your server's ID (optional, only for targeted slash command registration)
GUILD_ID=your-server-id

# Category ID where ticket channels will be created (or leave blank to auto-create)
TICKET_CATEGORY_ID=

# Log channel ID (for transcripts and events)
LOG_CHANNEL_ID=

# Role to give when user is verified
VERIFIED_ROLE_ID=

# Role to assign to new members (unverified)
UNVERIFIED_ROLE_ID=

# Tebex secret key (optional, for Tebex integration)
TEBEX_SECRET=your-tebex-secret-if-used
